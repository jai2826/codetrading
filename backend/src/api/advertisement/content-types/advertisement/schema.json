{
  "kind": "collectionType",
  "collectionName": "advertisements",
  "info": {
    "singularName": "advertisement",
    "pluralName": "advertisements",
    "displayName": "Advertisement",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "identifier": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "speciality": {
      "displayName": "speciality",
      "type": "component",
      "repeatable": true,
      "component": "points.speciality"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Null",
        "Monthly",
        "Semi-annually",
        "Annually"
      ]
    },
    "footer": {
      "type": "string"
    }
  }
}
